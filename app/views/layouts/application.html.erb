<!DOCTYPE html>
<html>
  <head>
    <%= display_meta_tags site: 'Auckland Ev', reverse: true %>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <link rel="stylesheet" href="https://use.typekit.net/vww5ope.css">
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>

  <body>
    <%= render 'menu' %>
    <%= yield %>
    <%= render 'footer' %>
  </body>
</html>
